<%= simple_form_for([@restaurant, @restaurant.reviews.build]) do |f|%>
<div id="star-form">
 <label> Rating </label>
</div>


<%= f.input :comment %>
<%= f.button :submit, class: "btn-custom2" %>
<% end %>

<script>
	$('#star-form').raty({
		path: '/assets/images/',
		scoreName: 'review[rating]'
	});
</script>